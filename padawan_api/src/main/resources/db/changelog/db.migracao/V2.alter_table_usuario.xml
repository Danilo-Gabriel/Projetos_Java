<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.10.xsd">


<!--  INSERINDO COLUNAS NA TABELA USUARIO -->

   
   <changeSet id="V2_alter_table_usuario" author="daniloGabriel"> 
    <addColumn tableName="usuarios">
        <column name="nome_completo" type="varchar(255)"></column>
        <column name="email" type="varchar(255)"></column>  
        <column name="cpf" type="char(11)"> </column>
        <column name="hash" type="varchar(255)"></column>
    </addColumn>  
</changeSet>

</databaseChangeLog>
